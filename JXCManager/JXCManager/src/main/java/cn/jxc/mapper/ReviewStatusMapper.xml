<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.jxc.mapper.ReviewStatusMapper">
	<resultMap type="ReviewStatus" id="ReviewStatusMap">
		<result property="rsId" column="RS_id"/>
		<result property="rsName" column="RS_name"/>
	</resultMap>
	<select id="getReviewStatus" resultMap="ReviewStatusMap" >
		SELECT * FROM `reviewstatus`
	</select>
</mapper>